<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>LastTask.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MyPageDataLoadServlet</servlet-name>
        <servlet-class>LastTask.MyPageDataLoadServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RoomMemoDataServlet</servlet-name>
        <servlet-class>LastTask.RoomMemoDataServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MemoDataChangeServlet</servlet-name>
        <servlet-class>LastTask.MemoDataChangeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>LastTask.RegisterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginCheck</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MyPageDataLoadServlet</servlet-name>
        <url-pattern>/MyPageData</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RoomMemoDataServlet</servlet-name>
        <url-pattern>/RoomMemoData</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MemoDataChangeServlet</servlet-name>
        <url-pattern>/MemoDataChange</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/Register</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
